	<body class='p-5'>
		<h1 class='text-3xl'>First Iteration</h1>
		<h2 class='py-6 font-bold'>Here you can find what zendesk issues are resolved</h2>
		<ul class='list-disc pl-5'>
			{{#each tasks}}
				{{#each external_links}}
					{{#isZendesk this}}
						<li>
							<a href='{{this}}'>zendesk issue nÂº {{extractId this}}</a>
						</li>
					{{/isZendesk}}
				{{/each}}
			{{/each}}
		</ul>

		<h2 class='py-6 font-bold'>Here what issues/features are in production</h2>
		<h3 class='font-bold text-xl text-red-900 py-3 px-2 rounded'>Bugs</h3>
		<ul class='list-disc pl-9'>
			{{#each tasks}}
				{{#isEqual this.story_type 'bug'}}
					<a href='{{this.app_url}}'>
						<li class='pb-1'>
							<span class='font-bold'>sc-{{this.id}}</span>
							<span>{{this.name}}</span>
						</li>
					</a>
				{{/isEqual}}
			{{/each}}
		</ul>
		<h3 class='font-bold text-xl text-blue-900 py-3 px-2 rounded'>Features</h3>
		<ul class='list-disc pl-9'>
			{{#each tasks}}
				{{#isEqual this.story_type 'feature'}}
					<a href='{{this.app_url}}'>
						<li class='pb-1'>
							<span class='font-bold'>sc-{{this.id}}</span>
							<span>{{this.name}}</span>
						</li>
					</a>
				{{/isEqual}}
			{{/each}}
		</ul>
		<h3 class='font-bold text-xl text-yellow-900 py-3 px-2 rounded'>Chore</h3>
		<ul class='list-disc pl-9'>
			{{#each tasks}}
				{{#isEqual this.story_type 'chore'}}
					<a href='{{this.app_url}}'>
						<li class='pb-1'>
							<span class='font-bold'>sc-{{this.id}}</span>
							<span>{{this.name}}</span>
						</li>
					</a>
				{{/isEqual}}
			{{/each}}
		</ul>
